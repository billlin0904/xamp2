#include <windows.h>

#ifndef VS_VERSION_INFO
#define VS_VERSION_INFO 1
#endif

#define _STRINGIFY(x) #x
#define STRINGIFY(x) _STRINGIFY(x)

// IDI_ICON1               ICON    DISCARDABLE     STRINGIFY()

VS_VERSION_INFO VERSIONINFO
    FILEVERSION    1,0,1,0
    PRODUCTVERSION 1,0,1,0
{
    BLOCK "StringFileInfo"
    {
       // U.S. English - Windows, Multilingual
       BLOCK "040904E4"
       {
          VALUE "FileDescription", STRINGIFY(Cross-platform window customization framework)
          VALUE "FileVersion", STRINGIFY(1.0.1.0)
          VALUE "ProductName", STRINGIFY(QWindowKit)
          VALUE "ProductVersion", STRINGIFY(1.0.1.0)
          VALUE "LegalCopyright", STRINGIFY(Copyright 2023 Stdware Collections)
        }
    }
    BLOCK "VarFileInfo"
    {
        VALUE "Translation", 0x409, 1252 // 1252 = 0x04E4
    }
}
