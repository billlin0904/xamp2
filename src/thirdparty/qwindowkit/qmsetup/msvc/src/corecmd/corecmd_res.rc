#include <windows.h>

#ifndef VS_VERSION_INFO
#define VS_VERSION_INFO 1
#endif

#define _STRINGIFY(x) #x
#define STRINGIFY(x) _STRINGIFY(x)

// IDI_ICON1               ICON    DISCARDABLE     STRINGIFY()

VS_VERSION_INFO VERSIONINFO
    FILEVERSION    0,0,1,5
    PRODUCTVERSION 0,0,1,5
{
    BLOCK "StringFileInfo"
    {
       // U.S. English - Windows, Multilingual
       BLOCK "040904E4"
       {
          VALUE "FileDescription", STRINGIFY(QMSetup Core Utility Command)
          VALUE "FileVersion", STRINGIFY(0.0.1.5)
          VALUE "ProductName", STRINGIFY(corecmd)
          VALUE "ProductVersion", STRINGIFY(0.0.1.5)
          VALUE "LegalCopyright", STRINGIFY(Copyright 2024 Stdware)
        }
    }
    BLOCK "VarFileInfo"
    {
        VALUE "Translation", 0x409, 1252 // 1252 = 0x04E4
    }
}
